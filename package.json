{
  "name": "moodyjw-portfolio",
  "version": "0.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch",
    "test:coverage": "ng test --no-watch --coverage",
    "test:watch": "ng test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:update-snapshots": "playwright test --update-snapshots",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint . --ext .ts,.html",
    "lint:fix": "eslint . --ext .ts,.html --fix",
    "storybook": "ng run moodyjw-portfolio:storybook",
    "build-storybook": "ng run moodyjw-portfolio:build-storybook",
    "docs": "compodoc -p tsconfig.json -d docs/compodoc"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "npm@10.8.2",
  "overrides": {
    "@ngrx/signals": {
      "@angular/core": "^21.0.0"
    },
    "@swimlane/ngx-charts": {
      "@angular/animations": "^21.0.0",
      "@angular/cdk": "^21.0.0",
      "@angular/common": "^21.0.0",
      "@angular/compiler": "^21.0.0",
      "@angular/core": "^21.0.0",
      "@angular/forms": "^21.0.0",
      "@angular/platform-browser": "^21.0.0",
      "@angular/platform-browser-dynamic": "^21.0.0"
    },
    "@storybook/angular": {
      "@angular-devkit/build-angular": "^21.0.0",
      "@angular/animations": "^21.0.0",
      "@angular/cli": "^21.0.0",
      "@angular/common": "^21.0.0",
      "@angular/compiler": "^21.0.0",
      "@angular/compiler-cli": "^21.0.0",
      "@angular/core": "^21.0.0",
      "@angular/forms": "^21.0.0",
      "@angular/platform-browser": "^21.0.0",
      "@angular/platform-browser-dynamic": "^21.0.0"
    },
    "webpack": "5.102.1"
  },
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/cdk": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/service-worker": "^21.0.0",
    "@jsverse/transloco": "^8.0.0",
    "@ng-icons/core": "^33.0.0",
    "@ng-icons/heroicons": "^33.0.0",
    "@ng-icons/ionicons": "^33.0.0",
    "@ngrx/signals": "^20.1.0",
    "@swimlane/ngx-charts": "^23.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zod": "^3.24.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.0",
    "@angular-eslint/builder": "^21.0.0",
    "@angular-eslint/eslint-plugin": "^21.0.0",
    "@angular-eslint/eslint-plugin-template": "^21.0.0",
    "@angular-eslint/template-parser": "^21.0.0",
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
